{
  "name": "media-processor",
  "version": "1.0.0",
  "main": "src/main.js",
  "license": "MIT",
  "scripts": {
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "build": "tsc",
    "build:watch": "onchange 'src/*.ts' 'src/**/*.ts' -- yarn run build",
    "start": "node ./build/index.js",
    "generate": "curl -O https://beta.frikanalen.no/api/v2/open-api-spec.json && openapi --input ./open-api-spec.json -c axios --output ./src/client",
    "generate-local": "curl -O http://localhost:8000/open-api-spec.json && openapi --input ./open-api-spec.json -c axios --output ./src/client",
    "dev": "ts-node-dev src/index.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.202.0",
    "@aws-sdk/lib-storage": "^3.202.0",
    "@koa/router": "^12.0.0",
    "@types/fluent-ffmpeg": "^2.1.17",
    "aws-sdk": "^2.1246.0",
    "axios": "^1.1.3",
    "bull": "^4.10.1",
    "dotenv": "^16.0.3",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "http-event-stream": "^0.2.0",
    "jest": "^29.2.2",
    "kafkajs": "^2.2.2",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-event-stream": "^1.0.3",
    "mime-types": "^2.1.35",
    "redis": "^4.4.0",
    "serve": "^14.0.1",
    "sharp": "^0.31.1",
    "small-uuid": "^1.0.1",
    "ts-node": "^10.9.1",
    "tslog": "^3.3.4",
    "typescript": "4.8.4",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/bull": "^4.10.0",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.9",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^18.11.9",
    "@types/sharp": "^0.31.0",
    "onchange": "^7.1.0",
    "openapi": "^1.0.1",
    "openapi-typescript-codegen": "^0.23.0",
    "prettier": "^2.7.1",
    "ts-node-dev": "^2.0.0"
  },
  "repository": "git@github.com:Frikanalen/media-processor.git",
  "author": "Tore Sinding Bekkedal <toresbe@protonmail.com>"
}

{
  "name": "media-processor",
  "version": "1.0.0",
  "license": "MIT",
  "exports": "./src/server.ts",
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "scripts": {
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAl l",
    "build": "tsc",
    "build:watch": "onchange 'src/*.ts' 'src/**/*.ts' -- yarn run build",
    "start": "NODE_ENV=production node --enable-source-maps ./build/server.js",
    "fetch-api": "curl -O https://beta.frikanalen.no/api/v2/open-api-spec.json",
    "fetch-local": "curl -O http://localhost:8080/open-api-spec.json",
    "generate": "openapi --input ./open-api-spec.json -c axios  --output src/generated && fix-esm-import-path src/generated && prettier -w src/generated >> /dev/null",
    "dev": "nodemon src/server.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.374.0",
    "@aws-sdk/lib-storage": "^3.374.0",
    "@koa/router": "^12.0.0",
    "aws-sdk": "^2.1421.0",
    "axios": "^1.4.0",
    "bullmq": "^4.6.0",
    "dotenv": "^16.3.1",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "http-event-stream": "^0.2.0",
    "ioredis": "^5.3.2",
    "jest": "^29.6.1",
    "kafkajs": "^2.2.2",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.4.1",
    "koa-event-stream": "^1.0.3",
    "mime-types": "^2.1.35",
    "prom-client": "^14.2.0",
    "redis": "^4.6.7",
    "serve": "^14.1.2",
    "sharp": "^0.32.4",
    "shell-quote": "^1.8.0",
    "small-uuid": "^1.0.1",
    "tslog": "^4.2.0",
    "yup": "^1.2.0"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^20.4.4",
    "fix-esm-import-path": "^1.4.0",
    "nodemon": "^3.0.1",
    "onchange": "^7.1.0",
    "openapi": "^1.0.1",
    "openapi-typescript-codegen": "^0.25.0",
    "prettier": "^3.0.0",
    "ts-node": "^10.9.1",
    "typescript": "5.1.6"
  },
  "repository": "git@github.com:Frikanalen/media-processor.git",
  "author": "Tore Sinding Bekkedal <toresbe@protonmail.com>"
}

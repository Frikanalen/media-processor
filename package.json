    "jest": "^27.0.6",
    "kafkajs": "^1.15.0",
    "mime-types": "^2.1.32",
    "serve": "^12.0.0",
    "small-uuid": "^1.0.1",
    "ts-node": "^10.0.0",
    "tslog": "^3.2.0",
    "typescript": "^4.3.5",
    "yup": "^0.32.11"
  },
  "devDependencies": {
    "@types/node": "^16.0.1",
    "onchange": "^7.1.0",
    "prettier": "^2.3.2"

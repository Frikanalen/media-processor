{
  "name": "media-processor",
  "version": "1.0.0",
  "license": "MIT",
  "exports": "./src/server.ts",
  "engines": {
    "node": ">=16"
  },
  "type": "module",
  "scripts": {
    "view:coverage": "serve coverage/lcov-report",
    "clean": "rm -rf coverage src/**/*.js src/**/*.map",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watchAll",
    "build": "tsc",
    "build:watch": "onchange 'src/*.ts' 'src/**/*.ts' -- yarn run build",
    "start": "node ./build/server.js",
    "fetch-api": "curl -O https://beta.frikanalen.no/api/v2/open-api-spec.json",
    "fetch-local": "curl -O http://localhost:8080/open-api-spec.json",
    "generate": "openapi --input ./open-api-spec.json -c axios  --output ./src/client && prettier -w src/client >> /dev/null",
    "dev": "nodemon src/server.ts"
  },
  "dependencies": {
    "@aws-sdk/client-s3": "^3.337.0",
    "@aws-sdk/lib-storage": "^3.337.0",
    "@koa/router": "^12.0.0",
    "aws-sdk": "^2.1382.0",
    "axios": "^1.4.0",
    "bull": "^4.10.1",
    "dotenv": "^16.0.3",
    "ffmpeg": "^0.0.4",
    "fluent-ffmpeg": "^2.1.2",
    "http-event-stream": "^0.2.0",
    "jest": "^29.3.1",
    "kafkajs": "^2.2.2",
    "koa": "^2.13.4",
    "koa-bodyparser": "^4.3.0",
    "koa-event-stream": "^1.0.3",
    "mime-types": "^2.1.35",
    "prom-client": "^14.2.0",
    "redis": "^4.6.6",
    "serve": "^14.1.2",
    "sharp": "^0.32.1",
    "shell-quote": "^1.8.0",
    "small-uuid": "^1.0.1",
    "tslog": "^4.2.0",
    "yup": "^1.1.1"
  },
  "devDependencies": {
    "@tsconfig/node18-strictest-esm": "^1.0.1",
    "@types/bull": "^4.10.0",
    "@types/fluent-ffmpeg": "^2.1.17",
    "@types/koa": "^2.13.5",
    "@types/koa-bodyparser": "^4.3.10",
    "@types/koa__router": "^12.0.0",
    "@types/node": "^20.2.3",
    "@types/sharp": "^0.32.0",
    "nodemon": "^2.0.20",
    "onchange": "^7.1.0",
    "openapi": "^1.0.1",
    "openapi-typescript-codegen": "^0.24.0",
    "prettier": "^2.7.1",
    "ts-node": "^10.9.1",
    "typescript": "5.0.4"
  },
  "repository": "git@github.com:Frikanalen/media-processor.git",
  "author": "Tore Sinding Bekkedal <toresbe@protonmail.com>"
}
